{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "Seq": {
      "type": "container.v0",
      "connectionString": "{Seq.bindings.http.url}",
      "image": "docker.io/datalust/seq:2025.2",
      "env": {
        "ACCEPT_EULA": "Y",
        "SEQ_FIRSTRUN_NOAUTHENTICATION": "True"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "targetPort": 80
        }
      }
    },
    "postgres": {
      "type": "container.v0",
      "connectionString": "Host={postgres.bindings.tcp.host};Port={postgres.bindings.tcp.port};Username=postgres;Password={postgres-password.value}",
      "image": "docker.io/ankane/pgvector:latest",
      "env": {
        "POSTGRES_HOST_AUTH_METHOD": "scram-sha-256",
        "POSTGRES_INITDB_ARGS": "--auth-host=scram-sha-256 --auth-local=scram-sha-256",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "{postgres-password.value}"
      },
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 5432
        }
      }
    },
    "identitydb": {
      "type": "value.v0",
      "connectionString": "{postgres.connectionString};Database=identitydb"
    },
    "identity-server": {
      "type": "project.v0",
      "path": "../Services/OroIdentityServer/OroIdentityServer.Server/OroIdentityServer.Server.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{identity-server.bindings.http.targetPort}",
        "ConnectionStrings__Seq": "{Seq.connectionString}",
        "SEQ_HOST": "{Seq.bindings.http.host}",
        "SEQ_PORT": "{Seq.bindings.http.port}",
        "SEQ_URI": "{Seq.bindings.http.url}",
        "ConnectionStrings__identitydb": "{identitydb.connectionString}",
        "IDENTITYDB_HOST": "{postgres.bindings.tcp.host}",
        "IDENTITYDB_PORT": "{postgres.bindings.tcp.port}",
        "IDENTITYDB_USERNAME": "postgres",
        "IDENTITYDB_PASSWORD": "{postgres-password.value}",
        "IDENTITYDB_URI": "postgresql://postgres:{postgres-password-uri-encoded.value}@{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/identitydb",
        "IDENTITYDB_JDBCCONNECTIONSTRING": "jdbc:postgresql://{postgres.bindings.tcp.host}:{postgres.bindings.tcp.port}/identitydb",
        "IDENTITYDB_DATABASENAME": "identitydb",
        "IDENTITY_WEB_HTTP": "{identity-web.bindings.http.url}",
        "services__identity-web__http__0": "{identity-web.bindings.http.url}",
        "IDENTITY_WEB_HTTPS": "{identity-web.bindings.https.url}",
        "services__identity-web__https__0": "{identity-web.bindings.https.url}",
        "IdentityWeb__Url": "{identity-web.bindings.https.url}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "identity-web": {
      "type": "project.v0",
      "path": "../Frontends/OroIdentity.Web/OroIdentity.Web.Server/OroIdentity.Web.Server.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{identity-web.bindings.http.targetPort}",
        "ConnectionStrings__Seq": "{Seq.connectionString}",
        "SEQ_HOST": "{Seq.bindings.http.host}",
        "SEQ_PORT": "{Seq.bindings.http.port}",
        "SEQ_URI": "{Seq.bindings.http.url}",
        "IDENTITY_SERVER_HTTP": "{identity-server.bindings.http.url}",
        "services__identity-server__http__0": "{identity-server.bindings.http.url}",
        "IDENTITY_SERVER_HTTPS": "{identity-server.bindings.https.url}",
        "services__identity-server__https__0": "{identity-server.bindings.https.url}",
        "Identity__Url": "{identity-server.bindings.https.url}",
        "IdentityWeb__Url": "{identity-web.bindings.https.url}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "identity-admin": {
      "type": "executable.v0",
      "workingDirectory": "../Frontends/identity-admin",
      "command": "npm",
      "args": [
        "run",
        "https"
      ],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "postgres-password": {
      "type": "parameter.v0",
      "value": "{postgres-password.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "secret": true,
          "default": {
            "generate": {
              "minLength": 22
            }
          }
        }
      }
    },
    "postgres-password-uri-encoded": {
      "type": "annotated.string",
      "value": "{postgres-password.value}",
      "filter": "uri"
    }
  }
}